{
  "name": "felin",
  "version": "0.2.2",
  "author": "Abdessamad Zgor <abdessamadzgor@gmail.com>",
  "license": "MIT",
  "type": "module",
  "source": "src/felin.ts",
  "module": "dist/felin.js",
  "types": "dist/felin.d.ts",
  "devDependencies": {
    "@parcel/packager-ts": "2.12.0",
    "@parcel/transformer-typescript-types": "2.12.0",
    "@types/node": "^20.12.12",
    "csstype": "^3.1.3",
    "esbuild": "^0.23.0",
    "jest": "^29.7.0",
    "parcel": "^2.12.0",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build-dev": "tsc && esbuild ./src/felin.js --format=esm --platform=node --keep-names --bundle --outfile=./dist/felin.js",
    "build": "parcel build",
    "build-tsc": "tsc",
    "prepublish": "yarn run build",
    "pretest": "node ./src/elements/generate.js && npm run build && npm link && npm link felin",
    "test": "jest; parcel examples/cats-blog/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abdessamad-zgor/felin.git"
  },
  "keywords": [
    "felin",
    "ui",
    "simple"
  ],
  "exports": {
    ".": "./dist/felin.js"
  },
  "publishConfig": {
    "files": [
      "dist/**",
      "package.json",
      "LICENCE",
      "README.md"
    ]
  }
}
