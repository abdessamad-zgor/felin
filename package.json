{
  "name": "felin",
  "version": "0.0.8",
  "main": "lib/felin.js",
  "author": "Abdessamad Zgor <abdessamadzgor@gmail.com>",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@types/node": "^20.12.12",
    "csstype": "^3.1.3",
    "esbuild": "^0.23.0",
    "parcel": "^2.12.0",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build-dev": "tsc && esbuild ./src/felin.js --format=esm --platform=node --keep-names --bundle --outfile=./dist/felin.js",
    "build": "tsc && esbuild ./src/felin.js --format=esm --platform=node --keep-names --bundle --minify --outfile=./dist/felin.js",
    "build-tsc": "tsc",
    "prepublish": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abdessamad-zgor/felin.git"
  },
  "keywords": [
    "felin",
    "ui",
    "simple"
  ],
  "exports": {
    ".": "./dist/felin.js"
  }
}
